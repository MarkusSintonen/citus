	)
	INSERT INTO modify_table (SELECT * FROM basic) RETURNING *
)
UPDATE modify_table SET val=-2 WHERE id IN (SELECT id FROM cte);
ERROR:  common table expressions are not supported in distributed modifications
WITH cte AS (
	WITH basic AS (
